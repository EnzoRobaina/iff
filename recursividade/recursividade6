#include <iostream>
#include <vector>
using namespace std;

//sem recursividade:
void imprimir(vector<int> n){
  cout<<endl<<"Vetor:"<<endl;
  //int s = sizeof(n)/sizeof(n[0]);
  for(int i = 0; i < n.size(); i++){
    cout<<n[i]<<endl;
  }
}

//com recursividade
void imprimir2(int n[]){
  
}

int main() {
vector<int> n;
int size = 0;

cout<<"Quantos números terá o seu vetor?"<<endl;
cin>>size;

for(int i = 0; i < size; i++){
  int num;
  cout<<"Digite o número da posição "<<i<<":";
  cin>> num;
  n.push_back(num);
}
  
imprimir(n);
return 0;
}
